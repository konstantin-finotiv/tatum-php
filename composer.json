{
    "name": "tatumio/tatum-php",
    "version": "2.0.0",
    "type": "library",
    "description": "Tatum offers a flexible framework to build, run, and scale blockchain apps fast with PHP!",
    "keywords": [
        "web3", "openapi", "php", "sdk", "tatum", "tatum.io",
        "blockchain", "bitcoin", "bitcoin-cash", "litecoin",
        "stellar", "xrp", "ethereum", "ETH", "tron", "nft"
    ],
    "homepage": "https://tatum.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "Tatum",
            "homepage": "https://tatum.io/",
            "role": "Team"
        },
        {   
            "name": "Mark Jivko",
            "homepage": "https://markjivko.com",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/tatumio/tatum-php/issues",
        "source": "https://github.com/tatumio/tatum-php"
    },
    "require": {
        "php": ">=7.3",
        "ext-dom": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-gmp": "*",
        "ext-bcmath": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpspec/prophecy-phpunit": "^2.0.1"
    },
    "autoload": {
        "psr-4": { "Tatum\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Tatum\\Test\\" : "test/" }
    }
}
